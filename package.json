{
	"name": "basement_manager",
	"description": "The main bot for my discord server. Currently just handles verification",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "StarnightFox",
	"license": "UNLICENSE",
	"type": "module",
	"imports": {
		"#root/*": [
			"./dist/*.js"
		],
		"#lib/*": [
			"./dist/lib/*.js"
		],
		"#structures/*": [
			"./dist/lib/structures/*.js"
		]
	},
	"dependencies": {
		"@sapphire/decorators": "^6.1.0",
		"@sapphire/discord-utilities": "^3.4.1",
		"@sapphire/discord.js-utilities": "7.3.0",
		"@sapphire/fetch": "^2.4.2",
		"@sapphire/framework": "^4.8.5",
		"@sapphire/plugin-editable-commands": "^3.1.0",
		"@sapphire/plugin-hmr": "^2.0.3",
		"@sapphire/plugin-logger": "^3.0.7",
		"@sapphire/plugin-subcommands": "^4.3.0",
		"@sapphire/time-utilities": "^1.7.12",
		"@sapphire/type": "^2.5.1",
		"@sapphire/utilities": "^3.17.0",
		"@skyra/env-utilities": "^1.3.0",
		"colorette": "^2.0.20",
		"discord.js": "^14.16.3",
		"eslint": "^8.57.1",
		"lint-staged": "^13.3.0",
		"pretty-quick": "^3.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.8.1",
		"@commitlint/config-conventional": "^17.8.1",
		"@sapphire/cli": "^1.9.3",
		"@sapphire/eslint-config": "^4.4.3",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^4.0.1",
		"@types/eslint": "^8.56.12",
		"@types/node": "^18.19.59",
		"@types/ws": "^8.5.12",
		"husky": "^8.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.10",
		"tsc-watch": "^6.2.0",
		"typescript": "^5.1.6"
	},
	"scripts": {
		"build": "tsc -b src",
		"watch": "tsc -b src -w",
		"start": "node dist/index.js",
		"dev": "run-s build start",
		"watch:start": "tsc-watch -b src --onSuccess \"node ./dist/index.js\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"postinstall": "husky install .github/husky",
		"clean": "rimraf dist/",
		"clean:build": "rimraf dist/ && tsc -b src",
		"lint": "eslint src --ext ts --fix"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.8.6"
}
